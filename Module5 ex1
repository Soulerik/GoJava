package Module5;

public class Array {
    public static void main( String[] args) {
        int[] myArray = {3, 7, 2, 5, 7, 11, 56, 3, 0, 76, 2, -4, -3};
        int max = 0;
        int min = 0;
        for(int temp : myArray){
            if(max < temp){ max = temp; }
        }
        for(int temp : myArray){
            if(min > temp){ min = temp; }
        }

        System.out.println("Max is " + max);
        System.out.println("Min is " + min);
    }
}
